{
  "name": "desafio-hacktudo-backend",
  "version": "1.0.0",
  "description": "Backend application for OLX hackaton",
  "main": "src/index.ts",
  "scripts": {
    "start": "yarn postinstall && node dist/src/index.js",
    "build": "tsc",
    "postinstall": "yarn build",
    "dev": "npx nodemon -L --exec ts-node --transpile-only --files src/index.ts",
    "lint": "npx eslint --fix \"**/*.{ts,tsx}\"",
    "typeorm": "npx ts-node ./node_modules/typeorm/cli.js",
    "docker": "docker-compose up"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/filipecancio/desafio-hacktudo-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/filipecancio/desafio-hacktudo-backend/issues"
  },
  "homepage": "https://github.com/filipecancio/desafio-hacktudo-backend#readme",
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.38"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.2",
    "nodemon": "^2.0.13",
    "prettier": "^2.4.1",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4"
  }
}
